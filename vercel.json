{
    "version": 2,
    "routes": [
      { "handle": "filesystem" },
      { "src": "/blog", "dest": "/blog.html" },
      { "src": "/blog/post_([1-5]).html", "dest": "/public/blog/post_$1.html" },
      { "src": "/images/(.*)", "dest": "/public/images/$1" },
      { "src": "/LiveData", "dest": "/LiveData.html" },
      { "src": "/", "dest": "/index.html" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }